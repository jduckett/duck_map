<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
<%= sitemap_content do |row| %>
  <url>
    <loc><%= row[:loc] %></loc>
    <lastmod><%= row[:lastmod] %></lastmod>
    <changefreq><%= row[:changefreq] %></changefreq>
    <priority><%= row[:priority] %></priority>
  </url>
<% end %>
</urlset>
